{
  "framework": "vite",
  "installCommand": "npm ci",
  "buildCommand": "npm run build -w bayou-help-client && node scripts/vercel-copy-client-dist.mjs",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
