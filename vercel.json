{
  "framework": "vite",
  "installCommand": "npm ci",
  "buildCommand": "npm run build -w client",
  "outputDirectory": "client/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
