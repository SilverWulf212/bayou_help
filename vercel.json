{
  "framework": "vite",
  "installCommand": "npm ci",
  "buildCommand": "node scripts/vercel-build.mjs",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}